{
	"compilerOptions": {
		"baseUrl": ".",
		"outDir": "dist",
		"allowJs": true,
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true,
		"importHelpers": true,
		"jsx": "react",
		"lib": [
		"dom",
		"es2015",
		"es2016",
		"es2017"
		],
		"skipLibCheck": true,
		"module": "es2015",
		"moduleResolution": "node",
		"noEmitHelpers": true,
		"noImplicitReturns": true,
		"noUnusedLocals": false,
		"noImplicitAny": false,
		"strictNullChecks": false,
		"strict": true,
		"target": "es2017"
	},

	"exclude": [
		"node_modules",
		"dist"
	],
	"types": [
	  "typePatches"
	]
}
